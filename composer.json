{
    "name": "ixocreate/cache",
    "description": "IXOCREATE Cache Library",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE",
            "email": "opensource@ixocreate.com",
            "homepage": "https://www.ixocreate.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "psr/container": "~1.0",
        "symfony/cache": "^5.2"
    },
    "require-dev": {
        "ext-pdo": "*",
        "doctrine/dbal": "^2.9",
        "ixocreate/coding-standard": "^2.0",
        "ixocreate/test": "^0.1.1"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Misc\\Cache\\": "tests/misc/",
            "Ixocreate\\Test\\Cache\\": "tests/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
